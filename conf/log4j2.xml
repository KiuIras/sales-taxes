<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
  <Appenders>
    <File name="file" fileName="salestaxes.log" immediateFlush="false" append="false">
      <PatternLayout pattern="%-5level %d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %c{1} - %msg%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="trace">
      <AppenderRef ref="file"/>
    </Root>
    <Logger name="net.sf.extjwnl" additivity="false" level="INFO">
      <AppenderRef ref="file"/>
    </Logger>
  </Loggers>
</Configuration>